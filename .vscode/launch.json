{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug ioq3_map_exporter",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/ioq3_map_exporter",
            "args": [
                "--base_path",
                "${workspaceFolder}/data",
                "--map",
                "q3dm1",
                "--output",
                "${workspaceFolder}/out"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}